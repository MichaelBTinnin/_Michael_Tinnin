<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TINNIN COFFEE ROASTERS</title>
    @*Styles.Render("~/Content/css")*@ 
    @Styles.Render("~/bundles/medernizer")
    <link href="~/Content/PageStyles.css" type="text/css" rel="stylesheet" />
</head>
<body>
    <div id="container">
        <div id="header">
            <h1>TINNIN COFFEE ROASTERS</h1>
        </div>
        <div id="LeftColumn">
            <div id="navigation">
