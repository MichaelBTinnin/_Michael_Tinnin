                    <li><a href="#">Next Item C</a></li>
                </ul>
            </div>
        </div>
        <div id="RightColumn">
            <div id="MainContent">
                @RenderBody()
            </div>
        </div>
        <div id="footer">
            Copyright info here
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>







