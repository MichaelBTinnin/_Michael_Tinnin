                    <li>@Html.ActionLink("Home", "Index", "Home")</li> @*Parameters, ("page","method","controller")*@
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Mail", "Index", "Mail")</li>
                    <li>@Html.ActionLink("Wage", "Index", "Wage")</li>
